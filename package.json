{
  "name": "visualize",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://part-io.github.io/visualize",
  "dependencies": {
    "@sitecore-jss/sitecore-jss": "^19.0.0",
    "@sitecore-jss/sitecore-jss-react": "^19.0.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.0.3",
    "@types/leaflet": "^1.7.6",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "apexcharts": "^3.32.0",
    "colord": "^2.9.2",
    "leaflet": "^1.7.1",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.2",
    "react-jss": "^10.8.2",
    "react-leaflet": "^3.2.2",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.4",
    "react-scrollama": "^2.2.16",
    "scroll-snap": "^3.2.4",
    "typescript": "^4.5.2",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "yarn prettier:fix && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,css,sass,scss,less,html,md}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,css,sass,scss,less,html,md}\"",
    "lint": "tsc --noEmit && eslint src/**/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "2.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx,js,json,css,sass,scss,less,html,md}": "yarn prettier:fix",
    "src/**/*.{ts,tsx}": "yarn lint --fix"
  }
}
